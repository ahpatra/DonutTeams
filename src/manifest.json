        {
            "configurationUrl": "https://donutteams.azurewebsites.net/configure",
            "canUpdateConfiguration": true,
            "scopes": [
                "team"
            ]
        }
    ],
    "staticTabs": [
        {
            "entityId": "com.contoso.helloworld.hellotab",
            "name": "Hello Tab",
            "contentUrl": "https://donutteams.azurewebsites.net/hello",
            "websiteUrl": "https://donutteams.azurewebsites.net/hello",
            "scopes": [
                "personal"
            ]
        }
    ],
    "bots": [
        {
            "botId": "5786f949-4443-4fb5-bfca-f616654bb656",
            "scopes": [
                "personal",
                "team"
            ],
            "supportsFiles": true,
            "isNotificationOnly": false
        }
    ],
    "composeExtensions": [
        {
            "botId": "8d5cffc5-2dbf-4464-b5d2-51b25fe9f3a1",
            "canUpdateConfiguration": true,
            "commands": []
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "donutteams.azurewebsites.net"
    ]
}